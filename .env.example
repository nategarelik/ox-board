# Environment Variables Template
# Copy this file to .env.local and update with your values

# Application URL
NEXT_PUBLIC_APP_URL=http://localhost:3000

# WebSocket URL for real-time features
NEXT_PUBLIC_WEBSOCKET_URL=ws://localhost:3000

# MediaPipe version
NEXT_PUBLIC_MEDIAPIPE_VERSION=0.4.0

# Stemify Backend API URL (for stem separation)
# Point this to your deployed backend or local instance
NEXT_PUBLIC_STEMIFY_API_URL=http://localhost:8000

# Vercel KV (Redis) - Optional, for production
# VERCEL_KV_URL=
# VERCEL_KV_REST_API_URL=
# VERCEL_KV_REST_API_TOKEN=
# VERCEL_KV_REST_API_READ_ONLY_TOKEN=